        <property name="categorie_datepublication" fieldname="categorie_datepublication" datatype="datetime" default=""/>
        <property name="categorie_datecreation" fieldname="categorie_datecreation" datatype="datetime" required="true"/>
        <property name="categorie_datemodification" fieldname="categorie_datemodification" datatype="datetime" default=""/>
